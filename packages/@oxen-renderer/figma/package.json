{
  "name": "@oxen-renderer/figma",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "dependencies": {
    "@oxen/fig": "workspace:*",
    "@oxen/zip": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "scripts": {
    "lint": "eslint src",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest --run --passWithNoTests --config ../../../vitest.config.ts '../../../packages/@oxen-renderer/figma/src'",
    "dev:demo": "vite --config demo/vite.config.ts",
    "snapshot:generate": "bun run scripts/generate-snapshots.ts"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./svg": {
      "types": "./src/svg/index.ts",
      "default": "./src/svg/index.ts"
    },
    "./react": {
      "types": "./src/react/index.ts",
      "default": "./src/react/index.ts"
    }
  },
  "devDependencies": {
    "@resvg/resvg-js": "^2.6.2"
  }
}
