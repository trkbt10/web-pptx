{
  "name": "@oxen-office/ooxml",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "dependencies": {
    "@oxen/color": "workspace:*",
    "@oxen-office/opc": "workspace:*",
    "@oxen/xml": "workspace:*",
    "@oxen/zip": "workspace:*"
  },
  "scripts": {
    "lint": "eslint src",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest --run --passWithNoTests --config ../../../vitest.config.ts '../../../packages/@oxen-office/ooxml/src'"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./domain/*": {
      "types": "./src/domain/*.ts",
      "default": "./src/domain/*.ts"
    },
    "./domain/drawing": {
      "types": "./src/domain/drawing/index.ts",
      "default": "./src/domain/drawing/index.ts"
    },
    "./domain/drawing/*": {
      "types": "./src/domain/drawing/*.ts",
      "default": "./src/domain/drawing/*.ts"
    },
    "./parser": {
      "types": "./src/parser/index.ts",
      "default": "./src/parser/index.ts"
    },
    "./parser/*": {
      "types": "./src/parser/*.ts",
      "default": "./src/parser/*.ts"
    },
    "./parser/*/*": {
      "types": "./src/parser/*/*.ts",
      "default": "./src/parser/*/*.ts"
    },
    "./parser/drawing-ml": {
      "types": "./src/parser/drawing-ml/index.ts",
      "default": "./src/parser/drawing-ml/index.ts"
    },
    "./parser/drawing-ml/*": {
      "types": "./src/parser/drawing-ml/*.ts",
      "default": "./src/parser/drawing-ml/*.ts"
    },
  }
}
