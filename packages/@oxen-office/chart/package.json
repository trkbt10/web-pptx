{
  "name": "@oxen-office/chart",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "dependencies": {
    "@oxen-office/ooxml": "workspace:*",
    "@oxen-office/xlsx": "workspace:*",
    "@oxen/xml": "workspace:*"
  },
  "scripts": {
    "lint": "eslint src",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest --run --passWithNoTests --config ../../../vitest.config.ts '../../../packages/@oxen-office/chart/src'"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./domain": {
      "types": "./src/domain/types.ts",
      "default": "./src/domain/types.ts"
    },
    "./domain/*": {
      "types": "./src/domain/*.ts",
      "default": "./src/domain/*.ts"
    },
    "./parser": {
      "types": "./src/parser/index.ts",
      "default": "./src/parser/index.ts"
    },
    "./parser/*": {
      "types": "./src/parser/*.ts",
      "default": "./src/parser/*.ts"
    },
    "./parser/*/*": {
      "types": "./src/parser/*/*.ts",
      "default": "./src/parser/*/*.ts"
    },
    "./patcher": {
      "types": "./src/patcher/index.ts",
      "default": "./src/patcher/index.ts"
    },
    "./patcher/*": {
      "types": "./src/patcher/*.ts",
      "default": "./src/patcher/*.ts"
    },
    "./patcher/*/*": {
      "types": "./src/patcher/*/*.ts",
      "default": "./src/patcher/*/*.ts"
    }
  }
}

