            "lineWidth": 5,
            "lineCompound": "double",
            "lineDash": "dash"
          }
        ]
      }
    ]
  },
  "expected": {
    "slides": [
      {
        "slideNumber": 1,
        "shapes": [
          { "index": -5, "type": "sp", "line": { "compound": "dbl" } },
          { "index": -4, "type": "sp", "line": { "compound": "tri" } },
          { "index": -3, "type": "sp", "line": { "compound": "thickThin" } },
          { "index": -2, "type": "sp", "line": { "compound": "thinThick" } },
          { "index": -1, "type": "sp", "line": { "compound": "dbl", "dashStyle": "dash" } }
